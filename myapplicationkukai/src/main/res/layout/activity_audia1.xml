<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.kkkcut.www.myapplicationkukai.AutoType.typeActivity">

    <LinearLayout
        android:gravity="center_vertical"
        android:background="#16A4FA"
        android:layout_width="match_parent"
        android:layout_height="60dp">
        <ImageView
            android:layout_marginLeft="5dp"
            android:src="@drawable/selection"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_marginLeft="10dp"
            android:text="@string/Choice"
            android:textStyle="bold"
            android:textSize="25sp"
            android:textColor="#090100"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/tv_name"
            android:text="@string/Choice"
            android:textStyle="bold"
            android:textSize="25sp"
            android:textColor="#090100"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content" />
        <Button
            android:layout_marginRight="2dp"
            android:paddingRight="5dp"
            android:textColor="#090100"
            android:textStyle="bold"
            android:gravity="right|center"
            android:text="Home"
            android:onClick="backHomes"
            android:background="@drawable/homeimage"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            />
       </LinearLayout>

    <com.kkkcut.www.myapplicationkukai.AutoType.ClearEditText1
        android:id="@+id/filter_edi1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dip"
        android:background="@drawable/search_bar_edit_selector"
        android:drawableLeft="@drawable/search_bar_icon_normal"
        android:hint="请输入关键字"
        android:singleLine="true"
        android:textSize="15.0dip" />
     <LinearLayout
         android:orientation="horizontal"
         android:layout_width="match_parent"
         android:layout_height="fill_parent">
        <FrameLayout
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="fill_parent" >
            <ListView
                android:id="@+id/country_lvcountry1"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="center"
                android:divider="@null" />
            <TextView
                android:id="@+id/title_layout_no_friend1"
                android:layout_width="fill_parent"
                android:layout_height="30dp"
                android:layout_gravity="right|top"
                android:background="#E0E0E0"
                android:gravity="center_vertical"
                android:text="没有匹配类型"
                android:textColor="#454545"
                />

            <LinearLayout
                android:id="@+id/title_layou1"
                android:layout_width="fill_parent"
                android:layout_height="30dp"
                android:layout_gravity="right|top"
                android:background="#00FFFFFF"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/title_layout_catalog1"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:background="#00FFFFFF"
                    android:paddingBottom="5dip"
                    android:paddingLeft="5dip"
                    android:paddingTop="5dip"
                    android:visibility="gone"
                    android:textColor="#454545" />
            </LinearLayout>

            <TextView
                android:id="@+id/dialog1"
                android:layout_width="80.0dip"
                android:layout_height="80.0dip"
                android:layout_gravity="center"
                android:background="@drawable/show_head_toast_bg"
                android:gravity="center"
                android:textColor="#ffffffff"
                android:textSize="30.0dip"
                android:visibility="invisible" />

            <com.kkkcut.www.myapplicationkukai.AutoType.SideBar1
                android:id="@+id/sidrbar1"
                android:layout_width="30.0dip"
                android:layout_height="fill_parent"
                android:layout_gravity="right|center" />
        </FrameLayout>

     </LinearLayout>


</LinearLayout>
