                    android:textSize="24sp" />

                <Button
                    android:id="@+id/btn_P"
                    android:layout_width="50dp"
                    android:layout_height="55dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="2dp"
                    android:background="#16A4FA"
