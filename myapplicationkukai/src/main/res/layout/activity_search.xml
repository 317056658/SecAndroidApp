<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll_search_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.kkkcut.www.myapplicationkukai.search.FrmKeySearchActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#16A4FA">

        <ImageView
            android:id="@+id/iv_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:src="@drawable/selection" />

        <TextView
            android:id="@+id/search_mode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/iv_img"
            android:text="Key Blank Search"
            android:textColor="#030401"
            android:textSize="24sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/input_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:paddingLeft="100dp"
            android:textColor="#030401"
            android:textSize="16sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:background="@drawable/btn_home1_bg_effect"
            android:gravity="right|center"
            android:paddingRight="4dp"
            android:text="Home"
            android:textAllCaps="false" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="#0A1B47"
            android:orientation="vertical"
            android:padding="5dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/et_text_input"
                    android:layout_width="208dp"
                    android:layout_height="40dp"
                    android:background="#ffffff"
                    android:hint="Key Blank"
                    android:inputType="textNoSuggestions"
                    android:maxLength="15" />
                <Button
                    android:id="@+id/btn_key_blank"
                    android:layout_width="77dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/key_blank_img_switching"
                    android:gravity="bottom|center"
                    android:text="Key Blank"
                    android:textAllCaps="false"
                    android:textColor="#ffffff"
                    android:textSize="10sp" />

                <Button
                    android:id="@+id/ic_card"
                    android:layout_width="76dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="2dp"
                    android:background="@drawable/keysearch_searchiccardimage"
                    android:gravity="bottom|center"
                    android:text="IC Card"
                    android:textAllCaps="false"
                    android:textColor="#ffffff"
                    android:textSize="10sp" />
            </LinearLayout>

            <GridLayout

                android:id="@+id/grid_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:columnCount="7"
                android:rowCount="7">

                <Button
                    android:id="@+id/btn_A"
                    android:layout_width="50dp"
                    android:layout_height="55dp"
                    android:background="#16A4FA"
                    android:text="A"
                    android:textColor="#ffffff"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/btn_B"
                    android:layout_width="50dp"
                    android:layout_height="55dp"
                    android:layout_marginLeft="2dp"
                    android:background="#16A4FA"
                    android:text="B"
                    android:textColor="#ffffff"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/btn_C"
                    android:layout_width="50dp"
                    android:layout_height="55dp"
                    android:layout_marginLeft="2dp"
                    android:background="#16A4FA"
                    android:text="C"
                    android:textColor="#ffffff"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/btn_D"
                    android:layout_width="50dp"
                    android:layout_height="55dp"
                    android:layout_marginLeft="2dp"
                    android:background="#16A4FA"
                    android:text="D"
                    android:textColor="#ffffff"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/btn_1"
                    android:layout_width="50dp"
                    android:layout_height="55dp"
                    android:layout_marginLeft="5dp"
                    android:background="#16A4FA"
                    android:text="1"
                    android:textColor="#ffffff"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/btn_2"
                    android:layout_width="50dp"
