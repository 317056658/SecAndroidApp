                    android:layout_marginTop="2dp"
                    android:background="#16A4FA"
                    android:text="6"
                    android:textColor="#ffffff"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/btn_I"
                    android:layout_width="50dp"
