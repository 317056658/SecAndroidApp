<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="240dp"
    android:layout_height="120dp"
    android:padding="6dp"
    android:orientation="vertical"
    android:background="@drawable/picturebox1image1_dlgspaceselectres"
    tools:context="com.kkkcut.www.myapplicationkukai.dialogActivity.SelectKeySpacesActivity">
    <TextView
        android:layout_gravity="center_horizontal"
        android:textColor="#000"
        android:textSize="20sp"
        android:text="The following space variation\nexists with this key profile.\nSelect the correct space."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <LinearLayout
        android:gravity="center_horizontal"
        android:id="@+id/ll_space_group"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </LinearLayout>
</LinearLayout>
