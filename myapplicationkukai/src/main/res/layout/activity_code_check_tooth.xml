            <LinearLayout
                android:padding="8dp"
                android:orientation="vertical"
                android:layout_width="360dp"
                android:layout_height="400dp">
                <TextView
                    android:text="Series:"
                    android:textColor="#090100"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/tv_series_content"
                    android:layout_width="match_parent"
                    android:layout_height="260dp" />
                <TextView
                    android:focusable="true"
                    android:textColor="#090100"
                    android:text="Bitting:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <EditText
                    android:maxLines="1"
                    android:maxLength="15"
                    android:id="@+id/et_show_bitting"
                    android:background="@drawable/view_border"
                    android:focusable="false"
                    android:layout_width="220dp"
                    android:layout_height="34dp" />
            </LinearLayout>
            <RelativeLayout
                android:paddingRight="8dp"
                android:paddingBottom="8dp"
                android:layout_gravity="bottom|right"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <Button
                    android:layout_marginBottom="20dp"
                    android:layout_above="@+id/btn_close_activity"
                    android:layout_alignLeft="@+id/btn_close_activity"
                    android:id="@+id/btn_back_data"
                    android:layout_width="90dp"
                    android:layout_height="60dp"
                    android:background="@drawable/btnenterimage"
                    android:gravity="bottom|center"
                    android:text="OK" />
                <Button
                    android:layout_alignParentRight="true"
                    android:layout_alignParentBottom="true"
                    android:id="@+id/btn_close_activity"
                    android:gravity="bottom|center"
                    android:background="@drawable/close_img_switching"
                    android:textAllCaps="false"
                    android:text="Close"
                    android:layout_width="90dp"
                    android:layout_height="60dp" />
            </RelativeLayout>
